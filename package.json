{
  "name": "fixmtoacrisconverter",
  "version": "1.0.1",
  "description": "FixM 4.2 50 ACRIS JSON converter .The is named as fixmtoacrisconverter.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "build": "npm run build-ts && copyfiles -u 1 src/asset/*/*.json lib/ && copyfiles -u 1 src/asset/*/*/*.json lib/",
    "clean": "shx rm -rf ./lib/*",
    "fixm": "tsc && node index.js",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@types/node": "^13.13.4",
    "@types/prettier": "^2.3.2",
    "@types/xml2js": "^0.4.9",
    "class-converter": "^1.2.0",
    "commander": "^8.2.0",
    "fs": "0.0.1-security",
    "iterate-object": "^1.3.4",
    "json-find": "^2.0.8",
    "logger": "0.0.1",
    "node-notifier": "^10.0.0",
    "object-scan": "^17.0.1",
    "prettier-ts": "^1.17.1-2",
    "typescript": "3.8.3",
    "util": "^0.12.4",
    "winston": "^3.3.3",
    "xml2js": "^0.4.23",
    "yarn": "^1.22.11"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fly2plan/fixm-to-acris-converter-lib.git"
  },
  "author": "Renjith K N .IBS software",
  "keywords": [
    "FIXM",
    "ACRIS"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fly2plan/fixm-to-acris-converter-lib.git"
  },
  "homepage": "https://github.com/fly2plan/fixm-to-acris-converter-lib.git#readme",
  "devDependencies": {
    "@irrelon/path": "^5.0.0",
    "@types/jest": "^27.0.1",
    "array-of-objects-functions": "^0.1.0",
    "copyfiles": "^2.4.1",
    "jest": "^27.2.0",
    "prettier": "^2.4.0",
    "shx": "^0.3.3",
    "ts-jest": "^27.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0"
  }
}
